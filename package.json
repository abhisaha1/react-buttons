    "babel-cli": "~6.18.0",
    "babel-core": "~6.18.2",
    "babel-eslint": "~7.1.1",
    "babel-loader": "~6.2.9",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.18.0",
    "babel-preset-react": "~6.16.0",
    "babel-preset-stage-0": "~6.16.0",
    "classnames": "~2.2.5",
    "coveralls": "~2.11.15",
    "css-loader": "~0.26.1",
    "eslint": "~3.11.1",
