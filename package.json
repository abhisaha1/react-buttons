{
  "name": "@trendmicro/react-buttons",
  "version": "0.3.0",
  "description": "Trend Micro Components: React Buttons",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "npm run lint && npm test && npm run build && npm run build-examples",
    "build": "rm -f lib/*; babel --out-dir ./lib ./src",
    "build-examples": "cd examples; webpack; cp -af index.html ../docs/index.html",
    "demo": "http-server -p 8000 docs/",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "./babel-tap --coverage test/*.js",
    "coveralls": "./babel-tap --coverage --coverage-report=text-lcov test/*.js | node_modules/.bin/coveralls",
    "webpack-dev-server": "cd examples; webpack-dev-server --hot --inline --host 0.0.0.0 --port 8000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trendmicro-frontend/react-buttons.git"
  },
  "author": "Tina C Lin (https://github.com/tinaclin)",
